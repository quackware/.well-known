{
  "$schema": "https://raw.githubusercontent.com/denoland/deno/main/cli/schemas/registry-completions.v2.json",
  "version": 2,
  "registries": [
    {
      "schema": "/:repository?",
      "variables": [
        {
          "key": "repository",
          "documentation": "https://github-intellisense.quack.rest/documentation/${repository}",
          "url": "https://github-intellisense.quack.rest/completions/${repository}"
        }
      ]
    },
    {
      "schema": "/:repository/:path*",
      "variables": [
        {
          "key": "repository",
          "documentation": "https://github-intellisense.quack.rest/documentation/${repository}",
          "url": "https://github-intellisense.quack.rest/completions/${repository}"
        },
        {
          "key": "path",
          "documentation": "https://github-intellisense.quack.rest/documentation/${repository}/${path}",
          "url": "https://github-intellisense.quack.rest/completions/${repository}/${path}"
        }
      ]
    }
  ]
}
